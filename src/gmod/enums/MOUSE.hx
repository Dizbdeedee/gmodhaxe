package gmod.enums;
/**
    Enumerations used by:
**/
@:native("_G")
extern enum abstract MOUSE(Int) {
    /**
        Mouse 4 button ( Sometimes, mouse wheel tilt left )
    **/
    var MOUSE_4;
    /**
        Mouse 5 button ( Sometimes, mouse wheel tilt right )
    **/
    var MOUSE_5;
    /**
        Mouse button count
    **/
    var MOUSE_COUNT;
    /**
        Mouse wheel scroll down
    **/
    var MOUSE_WHEEL_DOWN;
    /**
        First mouse button
    **/
    var MOUSE_FIRST;
    /**
        Middle mouse button, aka the wheel press
    **/
    var MOUSE_MIDDLE;
    /**
        Left mouse button
    **/
    var MOUSE_LEFT;
    /**
        Last mouse button
    **/
    var MOUSE_LAST;
    /**
        Right mouse button
    **/
    var MOUSE_RIGHT;
    /**
        Mouse wheel scroll up
    **/
    var MOUSE_WHEEL_UP;
    
}