package gmod.structs;
/**
    Table created by the [Color](https://wiki.garrysmod.com/page/Global/Color) function and used in various situations.
**/
typedef Color = {
    /**
        The red channel
    **/
    var r : Float;
    /**
        The green channel
    **/
    var g : Float;
    /**
        The blue channel
    **/
    var b : Float;
    /**
        The alpha channel
    **/
    var a : Float;
    
}
